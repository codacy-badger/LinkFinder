        "keywords": ["url", "link", "urlize", "urlizer", "email", "e-mail"],
        "homepage": "https://github.com/yarri/LinkFinder",
        "license": ["MIT"],
        "authors": [
                {
                        "name": "Jaromir Tomek",
                        "homepage": "https://github.com/yarri"
                }
        ],
        "require": {
                "php": ">=5.0.0"
        },
        "require-dev": {
                "phpunit/phpunit": "4.8.*"
        },
        "autoload": {
                "classmap": ["src"]
        }
}
